<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
           
           
    <!-- List of data access permissions -->
	<bean id="securityFilterMap" class="org.springframework.beans.factory.config.MapFactoryBean">
		<property name="sourceMap">	
			<map>
				<entry key="ACCESS_ALL"><value>1=1</value></entry>
			</map>
		</property>
	</bean>
	
	<bean id="evolveList" class="org.springframework.beans.factory.config.ListFactoryBean">	
		<property name="sourceList">
			<list />
		</property>
	</bean>
		
</beans>